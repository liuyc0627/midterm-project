uii <- fluidPage(
  ggplot(data = toxicity_val,
  aes(x = long, y = lat,
  group = group, fill = value))+
  geom_polygon(color = "gray90", size = 0.1) +
  coord_map(projection = "albers", lat0 = 39, lat1 = 45) +
  ggtitle("Map-average usaged of toxins")
)
server <- function(input, output, session) {
}    
shinyApp(uii, server)
